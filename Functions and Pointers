Question 5
#include <stdio.h>
void printPattern2(int height);
int main()
{
 int height;
 printf("Enter the height: \n");
 scanf("%d", &height);
 printf("printPattern2(): \n");
 printPattern2(height);
 return 0;
}
void printPattern2(int height)
{
    int i,j;
    for (i = 1; i<=height; i++)
    {
        for(j = i; j > 0; j--)
        {
            if(j % 2 == 0)
            {
                printf("B");
            }
            else if(j % 2 == 1)
            {
                printf("A");
            }
            
        }
        printf("\n");
    }

}




Question 6

#include <stdio.h>
void printPattern3(int height);
int main()
{
 int height;
 printf("Enter the height: \n");
 scanf("%d", &height);
 printf("printPattern3(): \n");
 printPattern3(height);
 return 0;
}
void printPattern3(int height)
{
    int i, j, s = 1;
    for ( i = 1; i<= height; i++)
    {
        s = i;
        for( j = 1; j<= i; j++ )
        {
            printf("%d",s);
            s++;
            if (s > 9 )
            {
                s = 0;
            }
            
        }
        printf("\n");
    }
}
